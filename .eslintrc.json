{
  "extends": "gatsby-standard",
  "rules": {
    "no-unsafe-finally": "off"
  }
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
            ["src", "./src"],
            ["pages", "./src/pages"],
            ["components", "./src/components"],
            ["lib", "./src/lib"],
            ["features", "./src/features"],
            ["resources", "./src/resources"],
            ["templates", "./src/templates"],
            ["ui", "./src/ui"]
         ],
         "extensions": [".js", ".jsx"]
      }
    }
}
}
